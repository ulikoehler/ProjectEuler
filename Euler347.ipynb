    "function M(p,q,N)\n",
    "    pq = p * q\n",
    "    pq > N || return 0\n",
    "    guess = pq * fld(N, pq)\n",
    "    for i = guess:1\n",
    "        ftors = factor(i)\n",
    "\n",
    "end"
   ]
