   "source": [
    "using Memoize\n",
    "coins = 1:99\n",
    "\n",
    "@memoize function count(n, m)\n",
    "    if n < 0 || m <= 0\n",
    "        return 0\n",
    "    elseif n == 0\n",
    "        return 1\n",
    "    end\n",
    " \n",
    "    return count( n, m - 1 ) + count( n - coins[m], m )\n",
    "end\n",
    "\n",
    "count(100, length(coins))"
   ]
  }
