   "source": [
    "coins = [1, 2, 5, 10, 20, 50, 100, 200]\n",
    "\n",
    "function count(n, m)\n",
    "    if n < 0 || m <= 0\n",
    "        return 0\n",
    "    elseif n == 0\n",
    "        return 1\n",
    "    end\n",
    " \n",
    "    return count( n, m - 1 ) + count( n - coins[m], m )\n",
    "end\n",
    "\n",
    "count(200, length(coins))"
   ]
  }
